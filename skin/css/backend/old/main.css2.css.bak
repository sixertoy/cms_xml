/*----------------------------------------------------------------------------
Generals
----------------------------------------------------------------------------*/

/* Links */
a{ color: #357; text-decoration: underline; }

a:focus,a:hover{ color: #357; }



/* -------------------------------------------------------------------
Typography 
------------------------------------------------------------------- */
h1{ font-size: 2.6em; }

h1 img,h2 img,h3 img,h4 img,h5 img,h6 img{ margin: 0; }

h1,h2,h3,h4,h5,h6{
	color: #444;
	font-weight: normal;
	line-height: 1;
	margin-bottom: 0.3em;
}

h2{ font-size: 2em; }

h3{ font-size: 1.5em; }

h4{ font-size: 1.25em; }

h4,h5,h6{ font-weight: bold; }

h5{ font-size: 1.1em; }

h6{ font-size: 1em; }

html{
	height: 100%;
	padding-bottom: 1px; /* force scrollbars */
}

/* Single line IE fix */
html #sub-nav,* html .section-title,* html .main{ height: 0.01%; min-height: 0.01%; }

/* Lists */
li ul,li ol{ margin-left: 1.5em; }

ol{ list-style-type: decimal; }

/* Tables */
table{ border-collapse: collapse; }

tfoot{ display: table-footer-group; }

thead{ display: table-header-group; }

ul{ list-style-type: disc; }

ul,ol{ margin: 0 0 1.5em 1.5em; }

ul.nice-list{ margin-left: 0; }

ul.nice-list li{
	list-style: none;
	border-top: 1px solid #EEE;
	padding: 4px 0;
}

ul.nice-list li .right{ color: #999; }

ul.nice-list li:first-child{ border-top: none; }

ul.plain-list{ margin: 0; }

/* Special lists */
ul.pretty-list li, ul.plain-list li,ul.nice-list li,ul.tabbed li{ list-style: none; }

ul.tabbed{ display: inline; margin: 0; }

ul.tabbed li{ float: left; }

.center-column{
	padding: 25px;
	margin: 0 20px;
	overflow: hidden;
	border-radius: 4px;
	-moz-border-radius: 4px;
	border: 1px solid #d7d7d7;
	-webkit-border-radius: 4px;
}

/*----------------------------------------------------------------------------
Classes
----------------------------------------------------------------------------*/
.center-wrapper{ margin: 0 auto; max-width: 1260px; min-width: 980px; }

.clear,.clearer{ clear: both; }

.clearer{
	display: block;
	font-size: 0;
	line-height: 0;
	height: 0;
}

.left,.alignleft{ float: left; }

.nav-divider{
	height: 40px;
	width: 15px;
	background: url(../img/shared/nav/divider.jpg) no-repeat top left;
}

.right,.alignright{ float: right; }

/*----------------------------------------------------------------------------
Section
----------------------------------------------------------------------------*/
.section{ width: 271px; margin-bottom: 16px; }

.section a{
	color: #94b52c;
	font-size: 11px;
	font-weight: bold;
	text-decoration: none;
}

.section a:hover{ text-decoration: underline; }

.section p{
	color: #b7b7b7;
	font-size: 11px;
	line-height: 0.9em;
	margin-bottom: 7px;
}

.section p:first-child /* titre de la section */{
	color: #272727;
	font-size: 13px;
	line-height: 1em;
	font-weight: bold;
}

.section-body{
	/*width: 259px;*/
	padding: 15px 0;
	/*margin-left: 5px;*/
	border: 1px solid #EAEAEA;
}

.section-body ul.pretty-list{
	padding-bottom: 5px;
	background: url( ../img/forms/lines_dotted_short.gif ) no-repeat center bottom;
}

.section-body ul.pretty-list li{
	padding-left: 30px;
	padding-bottom: 0.3em;
	background: url( ../img/forms/icon_plus.gif ) top left no-repeat;
}

.section-body ul.pretty-list li ul.pretty-list{ margin: 0; }

.section-body ul.pretty-list li ul.pretty-list li{
	line-height: 1.2em;
	padding-left: 15px;
	margin-bottom: 0.1em;
	background: url( ../img/forms/icon_list_arrow.gif ) no-repeat scroll 0 5px transparent;
}

.section-body ul:last-child.pretty-list,
.section-body ul.pretty-list li ul.pretty-list{ background: transparent; }

.section-title{
	height: 43px;
	/*background: url( ../img/forms/header_related_act.gif ) no-repeat top center;*/
	/*CSS3*/
	border: 1px solid #EAEAEA;
	border-bottom:0;
	border-radius: 7px 7px 0px 0px;
	background: linear-gradient( #636363, #414141 );
	background: -moz-linear-gradient( #636363, #414141 );/*firefox*/
	/*background: -webkit-linear-gradient( #636363, #414141 ); *//*safari*/
}

.side-column{ width: 20px; }

.step-dark-left{
	float: left;
	background: url(../img/forms/step_dark_left.gif) no-repeat;
	height: 27px;
	padding: 13px 15px 0 25px;
	font-size: 13px;
	color: #fff;
	font-weight: bold;
}

.step-dark-left a{ font-size: 13px; color: #fff; font-weight: bold; }

.step-dark-right{
	float: left;
	width: 12px;
	background: url(../img/forms/step_dark_right.gif) no-repeat;
	height: 39px;
}

.step-dark-round{
	float: left;
	width: 8px;
	height: 39px;
	background: url(../img/forms/step_dark_round.gif) no-repeat;
}

.step-light-left{
	float: left;
	height: 27px;
	padding: 13px 15px 0 25px;
	font-size: 13px;
	background: url(../img/forms/step_light_left.gif) no-repeat;
	height: 39px;
	color: #cbcbcb;
	font-weight: bold;
}

.step-light-left a{ font-size: 13px; color: #cbcbcb; font-weight: bold; }

.step-light-right{
	float: left;
	width: 12px;
	background: url(../img/forms/step_light_right.gif) no-repeat;
	height: 39px;
}

.step-light-round{
	float: left;
	width: 8px;
	height: 39px;
	background: url(../img/forms/step_light_round.gif) no-repeat;
}

.step-no{
	float: left;
	height: 29px;
	width: 18px;
	font-size: 18px;
	line-height: 18px;
	padding: 10px 0 0 0;
	text-align: center;
}

.step-no-off{
	float: left;
	height: 29px;
	width: 18px;
	font-size: 18px;
	line-height: 18px;
	padding: 10px 0 0 0;
	text-align: center;
	color: #cbcbcb;
}

/*----------------------------------------------------------------------------
Outters
----------------------------------------------------------------------------*/
#footer {
	margin: 20px auto 0;
	height: 70px;
	max-width: 1260px;
	min-width: 980px;
	text-align: left;
	padding: 0;
}

#footer div p {
	background: url( "../img/shared/bluemagic.jpg" ) no-repeat scroll 0 12px transparent;
	color: #818181;
	font-size: 11px;
	margin: 0 20px;
	padding: 15px 0 15px 25px;
	line-height: 11px;
	border-top: 1px solid #DBDBDB;
	border-bottom: 1px solid #DBDBDB;
}

#header{
	background: url( "../img/shared/top_bg.jpg" ) repeat-x scroll center top;
	padding: 15px 0;
}

#layout{
	background: url( "../img/shared/content_repeat.jpg" ) repeat-x scroll 0 0;
}

/*----------------------------------------------------------------------------
Layout
----------------------------------------------------------------------------*/
#layout-outter-content{ padding: 35px 0; line-height: 18px; }

#layout-three-columns{ min-width: 980px; max-width: 1260px; }

#main-nav{ left: 0; position: relative; margin: 0; padding: 0; }

#main-nav ul li span{
	display: none;
	position: absolute;
	left: 0;
	float: left;
	width: 980px;
}

#main-nav ul li span a{
	font-weight: bold;
	display: inline;
	background: transparent;
}

/*----------------------------------------------------------------------------
Sub Navigation
----------------------------------------------------------------------------*/
#main-nav ul li:hover span{ display: block; }

#main-nav ul li:hover span a:hover{ color: #94b52c; }

/*----------------------------------------------------------------------------
Navigation
----------------------------------------------------------------------------*/
#main-nav ul.plain-list li{ float: left; }

#main-nav ul.tabbed li a{
	/*background:url( "../img/shared/nav/pro_line_0x.gif" ) repeat scroll 0 0 transparent;*/
	: ;
}

#main-nav ul.tabbed li:hover a b,
#main-nav ul.tabbed li a:hover b{
	background: url(../img/shared/nav/pro_line_1.gif) right top;
	display: block;
	padding: 0 20px 0 10px;
}

#main-nav ul.tabbed li:hover a,
#main-nav ul.tabbed li a:hover{
	background: url(../img/shared/nav/pro_line_1.gif);
	color: #fff;
	padding: 0 0 0 10px;
}

#main-nav ul.tabbed li:hover span a{ padding: 0 20px; background: transparent; }



#navigation a{
	display: block;
	text-decoration: none;
	color: #FFF;
	height: 37px;
	cursor: pointer;
	padding: 0 20px;
	line-height: 35px;
}

#page-heading{ margin: 0 0 15px 0px; padding-left: 20px; }

#page-heading h1{ line-height: 24px; font-size: 22px; color: #4B4B4B; }

#sidebar{ width: 271px; margin-left: 30px; }

#single-column-render div#render{ width: 100%; }

/*----------------------------------------------------------------------------
Header
----------------------------------------------------------------------------*/
#site-title{ padding-left: 20px; }

/* -------------------------------------------------------------------
Breadscrumb
------------------------------------------------------------------- */
#step-holder{ font-family: Tahoma; height: 39px; margin-bottom: 20px; }

#two-column-render div#render{ margin: 0 auto; min-width: 585px; max-width: 800px; }